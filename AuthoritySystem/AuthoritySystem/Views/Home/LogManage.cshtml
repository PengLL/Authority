@model Domain.Model.Log

@using Lib;
@{
    ViewBag.Title = "LogManage";
}
<div class="ContentPanel">
    @Html.ValidationSummary()
@using (Html.BeginForm("LogManage", "Home"))
{
    <table class="table table-bordered">
        <tr style="display:none;"><td>用户Id</td><td><input name="User_Id" value="@ViewBag.UserId"/></td></tr>
        <tr><td>用户</td><td>@ViewBag.UserName</td></tr>
        <tr><td>时间</td><td><input name="Date" value="@ViewBag.Date" style="width:98%;"/></td></tr>
        <tr><td>内容</td><td><textarea name="Content" style="width:98%"></textarea></td></tr>
    </table>
    <button type="submit" class="btn btn-default">提交</button>
}
    
</div>